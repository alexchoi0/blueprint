print("=== Testing lazy map/filter ===")

print("\n1. Basic map (returns generator):")
result = map(lambda x: x * 2, [1, 2, 3, 4, 5])
print("type(map(...)):", type(result))

print("\n2. Iterate over map result:")
for x in map(lambda x: x * 2, [1, 2, 3]):
    print("  ", x)

print("\n3. Convert map to list:")
doubled = list(map(lambda x: x * 2, [1, 2, 3, 4, 5]))
print("list(map(...)):", doubled)

print("\n4. Basic filter:")
evens = list(filter(lambda x: x % 2 == 0, [1, 2, 3, 4, 5, 6]))
print("filter evens:", evens)

print("\n5. filter with None (truthy filter):")
truthy = list(filter(None, [0, 1, "", "hello", None, True, False]))
print("truthy values:", truthy)

print("\n6. Chained map/filter (lazy):")
result = filter(lambda x: x > 5, map(lambda x: x * 2, [1, 2, 3, 4, 5]))
print("type of chained:", type(result))
print("materialized:", list(result))

print("\n7. Index error on generator - skipped (no try/except)")

print("\n8. Lazy evaluation proof:")
call_count = [0]
def track(x):
    call_count[0] = call_count[0] + 1
    print("  processing:", x)
    return x * 2

gen = map(track, [1, 2, 3, 4, 5])
print("After map(), call_count:", call_count[0])

print("Taking first 2 items:")
for i, x in enumerate(gen):
    print("    got:", x)
    if i >= 1:
        break

print("Final call_count:", call_count[0])

print("\n=== All tests passed! ===")
