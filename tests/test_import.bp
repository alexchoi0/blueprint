load("./lib_example.bp", "PUBLIC_VALUE", "public_list", "public_func", "get_public_list")

assert(PUBLIC_VALUE == 42, "PUBLIC_VALUE should be 42")
assert(len(public_list) == 3, "public_list should have 3 elements")
assert(public_list[0] == 1, "public_list[0] should be 1")
assert(public_func() == "public function result", "public_func should return correct value")
